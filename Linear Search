

#include <stdio.h>

int main()

{
    
int a[50],n,i,flag=0,key;
    
printf("Enter how many element you want in array");
    
scanf("%d",&n);
    
printf("Emter n no.");
    
for(i=0;i<n;i++)
    
scanf("%d",&a[i]);
    
printf("Enter key no");
    
scanf("%d",&key);
    
for(i=0;i<n;i++)
    
{
        
if(a[i]==key)
        
{
            
flag=1;
            
break;
        
}
    
}
    
if(flag==0)
    
{
        
printf("no is not found");
    
}
    
else
    
{
        
printf("no is found");
    
}

}

